{
  "manifest_version": 2,

  "name": "Exa AI Plugin",
  "description": "This extension will query exa ai signals",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://exacoin.co/*"],
      "js": ["index.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "tabs"
  ]
}